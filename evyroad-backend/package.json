    "joi": "^17.9.2",
    "knex": "^2.5.1",
    "pg": "^8.11.3",
    "redis": "^4.6.7",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1427.0",
    "stripe": "^12.18.0",
