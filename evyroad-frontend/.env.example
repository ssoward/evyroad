# EvyRoad Frontend Environment Variables
# IMPORTANT: Copy this file to .env and update with your actual values
# Frontend .env files are exposed to the browser - only include public configuration

# API Configuration
VITE_API_URL=http://localhost:3001/api/v1
VITE_API_TIMEOUT=10000

# Map Configuration
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_DEFAULT_MAP_CENTER_LAT=40.7128
VITE_DEFAULT_MAP_CENTER_LNG=-74.0060
VITE_DEFAULT_MAP_ZOOM=10

# App Configuration
VITE_APP_NAME=EvyRoad
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Motorcycle Trip Tracking Platform

# External Services (Public Keys Only)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_CHAT_SUPPORT=false
VITE_ENABLE_BETA_FEATURES=false

# Environment
VITE_ENVIRONMENT=development
VITE_APP_VERSION=1.0.0
